<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.zerock.fc.dao.TimeMapper"> <!--클래스이름-->
    <select id="getTime" resultType="string"> <!-- id : 메서드이름 / resultType : 타입설정(자동으로 만들어줌)(원래 풀네임(java.lang.string?) string으로 줄일수있음) -->
        select now()
    </select>
</mapper>